function dydt = RobotODE(t, y, F)
%     y represents a vector of four states of the system.
%     F is the external force applied to the system.
    x = y(1);
    x_dot = y(2);
    theta = y(3);
    theta_dot = y(4);
    D = m^2*L^2*(cos(theta)^2-1) - (M+m)*I - M*m*L^2;
    
    dydt = zeros(4, 1);
    
    dydt(1) = x_dot;
    dydt(3) = theta_dot;
    dydt(4) = 1/D*(m*g*L*(M+m)*sin(theta) - m*L*cos(theta)*b*x_dot ...
        + m^2*L^2*theta_dot^2*sin(theta)*cos(theta) + m*L*cos(theta)*F);
    dydt(2) = -(I+m*L^2)/(m*L*cos(theta))*dydt(4) - g*sin(theta)/cos(theta);
end

